def count_even_odd(numbers):
    even_count = 0
    odd_count = 0
    for num in numbers: 
        if num % 2 == 0:
            even_count += 1
        else:
            odd_count += 1
            return even_count,odd_count
        #example series of numbers 
        numbers = [12,7,3,8,10,15,6]
even_count,odd_count=1
count_even_odd("numbers")
print("numbers of even numbers:",even_count)
print("numbers of odd numbers:",odd_count)
